<%= form_for(@em_service) do |f| %>

  <div class="field">
    <%= f.label :title, "标题" %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content, "内容" %><br>
    <%= f.kindeditor :content, height: 500 %>
  </div>
  <div class="actions text-right mt1em">

    <%= f.submit "提交",class: "button small" %>
  </div>
<% end %>
