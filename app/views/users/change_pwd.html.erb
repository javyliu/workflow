<%= form_for(@user) do |f| %>
  <div class="field">
    <p><strong>姓名: <%= f.object.user_name %> </strong></p>
  </div>
  <div class="field">
    <%= f.label :password,"密码" %>
    <%= f.password_field :password %>
  </div>
  <div class="field">
    <%= f.label :password_confirmation,"密码确认" %>
    <%= f.password_field :password_confirmation %>
  </div>
  <div class="actions">
    <%= f.submit "提交",class: "button" %>
  </div>
<% end %>



