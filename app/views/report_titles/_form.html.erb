<%= form_for(@report_title) do |f| %>
  <div class="field">
    <%= f.label :des,"名称" %><br>
    <%= f.text_field :des %>
  </div>
  <div class="field">
    <%= f.label :ord,"排序" %><br>
    <%= f.number_field :ord %>
  </div>
  <div class="actions">
    <%= f.submit "提交",class:"button small" %>
  </div>
<% end %>
