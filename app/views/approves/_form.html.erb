<%= form_for(@approve) do |f| %>
  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.number_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </div>
  <div class="field">
    <%= f.label :state %><br>
    <%= f.number_field :state %>
  </div>
  <div class="field">
    <%= f.label :des %><br>
    <%= f.text_field :des %>
  </div>
  <div class="field">
    <%= f.label :episode_id %><br>
    <%= f.number_field :episode_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
