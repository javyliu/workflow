  <%= form_for(@journal) do |f| %>
<div class="row">
  <div class="columns medium-6">
    <%= f.label :update_date,"日期" %>
    <%= f.text_field :update_date,class: "datepicker",readonly:true %>
  </div>
  <div class="columns medium-6">
    <%= f.label :user_id,"用户" %>
    <%= f.select :user_id,@managed_user_list %>
  </div>
</div>
<div class="row">
  <div class="columns medium-6">
    <%= f.label :check_type,"异常类别" %>
    <%= f.select :check_type,Journal::CheckType[0...-1].map{|item| [item.third,item.second]} %>
  </div>
  <div class="columns medium-6">
    <div class="row collapse">
      <%= f.label :dval,"数值" %>
      <div class="columns small-8">
          <%= f.text_field :dval %>
      </div>
      <div class="columns small-4">
        <span class="postfix">点/天/次/小时</span>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="columns medium-6">
    <%= f.label :description,"描述" %>
    <%= f.text_area :description %>
  </div>
  <div class="columns medium-6">
    <label for="">&nbsp;</label>
    <%= f.submit "提交",class: "button small",data:{disable_with: '提交中...'} %>
  </div>
</div>
  <% end %>
