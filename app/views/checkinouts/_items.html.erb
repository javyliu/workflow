<% items.each do |item| %>
<tr>
  <td><%= item.rec_date %></td>
  <td><%= item.user_name %></td>
  <td><%= item.dept_name %></td>
  <td><%= item.checkin %></td>
  <td><%= item.checkout %></td>
</tr>
<% end %>
<tr>
  <td colspan="3"><%= paginate(items) %></td>
    <td colspan=2><%= page_entries_info items,entry_name: "记录" %></td>
</tr>
